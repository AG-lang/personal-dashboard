{\n  \"$schema\": \"https://turbo.build/schema.json\",\n  \"tasks\": {\n    \"build\": {\n      \"dependsOn\": [\"^build\"],\n      \"outputs\": [\".next/**\", \"!.next/cache/**\"]\n    },\n    \"dev\": {\n      \"cache\": false,\n      \"persistent\": true\n    },\n    \"start\": {\n      \"dependsOn\": [\"build\"]\n    },\n    \"lint\": {\n      \"dependsOn\": [\"^lint\"]\n    },\n    \"lint:fix\": {\n      \"dependsOn\": [\"^lint:fix\"]\n    },\n    \"type-check\": {\n      \"dependsOn\": [\"^type-check\"]\n    },\n    \"clean\": {\n      \"cache\": false\n    },\n    \"test\": {\n      \"dependsOn\": [\"^build\"]\n    }\n  }\n}