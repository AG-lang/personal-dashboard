{
  "buildCommand": "cd frontend && pnpm build",
  "outputDirectory": "frontend/.next", 
  "installCommand": "cd frontend && pnpm install",
  "functions": {
    "api/app/main.py": {
      "runtime": "python3.11"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/app/main.py"
    }
  ]
}